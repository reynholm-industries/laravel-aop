{
    "name": "reynholm/laravel-repositories",
    "description": "Provides an easy way to create your laravel repositories",
    "authors": [
        {
            "name": "Carlos Goce",
            "email": "carlosgoce@gmail.com",
            "homepage": "http://www.carlosgoce.com",
            "role": "Developer"
        }
    ],
    "license": [
        "MIT"
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4.*",
        "illuminate/database": "4.*"
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.3",
        "satooshi/php-coveralls": "dev-master",
        "codeception/aspect-mock": "0.4.2"
    },
    "autoload": {
        "psr-0": {
            "Reynholm\\LaravelAop": "src/"
        },
        "classmap": ["tests/"]
    },
    "minimum-stability": "stable"
}
